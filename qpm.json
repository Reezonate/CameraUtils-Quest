{
  "$schema": "https://raw.githubusercontent.com/QuestPackageManager/QPM.Package/refs/heads/main/qpm.schema.json",
  "version": "0.1.0",
  "sharedDir": "shared",
  "dependenciesDir": "extern",
  "info": {
    "name": "CameraUtils",
    "id": "camera-utils",
    "version": "1.1.10",
    "url": "https://github.com/Reezonate/CameraUtils-Quest",
    "additionalData": {
      "overrideSoName": "libcamera-utils.so"
    }
  },
  "workspace": {
    "scripts": {
      "build": [
        "pwsh build.ps1 $0:?"
      ],
      "clean": [
        "qpm s build -- -clean"
      ],
      "close": [
        "pwsh close-game.ps1 $0:?"
      ],
      "copy": [
        "pwsh copy.ps1 $0:?"
      ],
      "log": [
        "pwsh start-logging.ps1 $0:?"
      ],
      "qmod": [
        "pwsh buildQMOD.ps1 camera-utils"
      ],
      "restart": [
        "pwsh restart-game.ps1 $0:?"
      ]
    },
    "ndk": "^27.3.13750724",
    "qmodIncludeDirs": [
      "extern/libs",
      "build"
    ],
    "qmodIncludeFiles": [],
    "qmodOutput": "camera-utils.qmod"
  },
  "dependencies": [
    {
      "id": "beatsaber-hook",
      "versionRange": "^6.4.2",
      "additionalData": {}
    },
    {
      "id": "scotland2",
      "versionRange": "^0.1.6",
      "additionalData": {
        "includeQmod": false,
        "private": true
      }
    },
    {
      "id": "bs-cordl",
      "versionRange": "4008.*",
      "additionalData": {}
    },
    {
      "id": "custom-types",
      "versionRange": "^0.18.3",
      "additionalData": {}
    },
    {
      "id": "paper2_scotland2",
      "versionRange": "^4.6.4",
      "additionalData": {}
    }
  ]
}
